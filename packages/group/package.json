{
    "name": "@semaphore-anchor/group",
    "version": "0.0.1",
    "description": "A library to create and manage Semaphore-anchor groups.",
    "license": "MIT",
    "main": "dist/index.node.js",
    "exports": {
        "import": "./dist/index.mjs",
        "require": "./dist/index.node.js"
    },
    "types": "dist/types/index.d.ts",
    "files": [
        "dist/",
        "src/",
        "LICENSE",
        "README.md"
    ],
    "repository": "https://github.com/webb-tools/semaphore-anchor.git",
    "homepage": "https://github.com/webb-tools/semaphore-anchor/tree/main/packages/group",
    "scripts": {
        "build:watch": "rollup -c rollup.config.ts -w --configPlugin typescript",
        "build": "rimraf dist && rollup -c rollup.config.ts --configPlugin typescript",
        "prepublishOnly": "yarn build",
        "docs": "typedoc src/index.ts --out ../../docs/group"
    },
    "publishConfig": {
        "access": "public"
    },
    "devDependencies": {
        "rollup": "^2.77.2",
        "rollup-plugin-cleanup": "^3.2.1",
        "rollup-plugin-typescript2": "^0.31.2",
        "typedoc": "^0.22.11",
        "typescript": "^4.5.5"
    },
    "dependencies": {
        "@ethersproject/abi": "^5.6.4",
        "@ethersproject/bytes": "^5.6.1",
        "@ethersproject/providers": "^5.6.8",
        "@openzeppelin/contracts": "^4.4.2",
        "@zk-kit/incremental-merkle-tree": "0.4.3",
        "@zk-kit/incremental-merkle-tree.sol": "^1.0.0",
        "circomlibjs": "0.0.8"
    }
}
